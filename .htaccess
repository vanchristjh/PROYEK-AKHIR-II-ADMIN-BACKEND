# PHP Configuration for larger file uploads
<IfModule mod_php7.c>
    php_value upload_max_filesize 20M
    php_value post_max_size 21M
    php_value max_execution_time 300
    php_value max_input_time 300
</IfModule>

<IfModule mod_php8.c>
    php_value upload_max_filesize 20M
    php_value post_max_size 21M
    php_value max_execution_time 300
    php_value max_input_time 300
</IfModule>

